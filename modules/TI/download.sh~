#!/bin/bash

data_config=$( cat config.json )
eval "ti_folder=$( echo ${data_config} | jq -r ".TI_path" )"

if [ $( ls ${ti_folder}/feed | wc -l ) -eq 0 ]
then
    cd ${ti_folder}/feed
    echo "I am in $( pwd )"
    for link in $( cat ../TI_feeds.csv | cut -d',' -f 1 )
    do
	if [[  ${link} =~ ^https ]]
	then
	    file_name=$( basename ${link} )
	    if [ ! -f ${file_name} ]
	    then
		wget ${link}
	    fi
	fi
    done

    # echo ">>>>>>>>>>>>>>>>>>>>>>> abuseIPDB"
    # python3 ../abuseIPDB.py

fi
